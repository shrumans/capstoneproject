pipeline {
    agent any

    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "MAVEN_HOME"
    }

    stages {
        
         stage('Preperation') {
            steps {
                // Get some code from a GitHub repository
                git branch:'master',url:'https://github.com/shrumans/capstoneproject.git'

            }
         }
        stage('Build') {
            steps {
               bat "mvn clean test"
            }
           
        }
    }
   
}